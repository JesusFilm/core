{
  "files.eol": "\n",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "db",
      "port": 5432,
      "name": "users",
      "driver": "PostgreSQL",
      "database": "users",
      "username": "postgres",
      "password": "postgres"
    },
    {
      "previewLimit": 50,
      "server": "db",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "tags",
      "database": "tags",
      "username": "postgres"
    },
    {
      "previewLimit": 50,
      "server": "db",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "journeys",
      "database": "journeys",
      "username": "postgres"
    },
    {
      "previewLimit": 50,
      "server": "db",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "languages",
      "database": "languages",
      "username": "postgres"
    },
    {
      "previewLimit": 50,
      "server": "db",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "videos",
      "database": "videos",
      "username": "postgres"
    },
    {
      "previewLimit": 50,
      "server": "db",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "plausible_db",
      "database": "plausible_db",
      "username": "postgres"
    }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": ["javascript", "typescript"],
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "eslint.runtime": "node",
  "eslint.useFlatConfig": true,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.tsserver.watchOptions": {
    "excludeDirectories": [
      "**/node_modules",
      "**/dist",
      "**/.next",
      "**/coverage",
      "**/.cache",
      "**/.nx",
      "**/out-tsc",
      "**/.docusaurus",
      "**/test-results",
      "**/playwright-report"
    ]
  },
  "prettier.prettierPath": "./node_modules/prettier",
  "remote.localPortHost": "allInterfaces",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/coverage/**": true,
    "**/.cache/**": true,
    "**/dist/**": true,
    "**/.next/**": true,
    "**/.nx/**": true,
    "**/out-tsc/**": true,
    "**/.docusaurus/**": true,
    "**/test-results/**": true,
    "**/playwright-report/**": true,
    "**/playwright/.cache/**": true,
    "**/*.tsbuildinfo": true,
    "**/.tsbuildinfo": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.next": true,
    "**/coverage": true,
    "**/.cache": true,
    "**/.nx": true,
    "**/out-tsc": true,
    "**/.docusaurus": true,
    "**/test-results": true,
    "**/playwright-report": true,
    "**/playwright/.cache": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/.tsbuildinfo": true,
    "**/*.tsbuildinfo": true,
    "**/*.bundle.js": true,
    "**/*.chunk.js": true,
    "**/*.log": true
  },
  "files.exclude": {
    "**/.git": false,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["className\\s*:\\s*['\"]([^'\"]*)['\"]"]
  ],
  "git.enabled": true,
  "git.autoRepositoryDetection": false,
  "git.scanRepositories": [],
  "git.decorations.enabled": true,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true
}
