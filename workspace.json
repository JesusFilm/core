{
  "version": 2,
  "projects": {
    "api-gateway": {
      "root": "apps/api-gateway",
      "sourceRoot": "apps/api-gateway/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-gateway",
            "main": "apps/api-gateway/src/main.ts",
            "tsConfig": "apps/api-gateway/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-gateway/src/environments/environment.ts",
                  "with": "apps/api-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-gateway:build"
          }
        },
        "serve-all": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx serve api-journeys"
              },
              {
                "command": "nx serve api-gateway"
              }
            ],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-gateway/**/*.ts"]
          }
        },
        "generate-graphql": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "rover supergraph compose --config apps/api-gateway/supergraph.yml > apps/api-gateway/schema.graphql"
              }
            ]
          }
        }
      }
    },
    "api-journeys": {
      "root": "apps/api-journeys",
      "sourceRoot": "apps/api-journeys/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-journeys",
            "main": "apps/api-journeys/src/main.ts",
            "tsConfig": "apps/api-journeys/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-journeys/src/environments/environment.ts",
                  "with": "apps/api-journeys/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-journeys:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-journeys/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-journeys"],
          "options": {
            "jestConfig": "apps/api-journeys/jest.config.js",
            "passWithNoTests": true
          }
        },
        "generate": {
          "executor": "@nx-tools/nx-prisma:generate",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "migrations": {
          "executor": "@nx-tools/nx-prisma:migrations",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "rollback": {
          "builder": "@nx-tools/nx-prisma:rollback",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "seed": {
          "builder": "@nx-tools/nx-prisma:seed",
          "options": {
            "script": "apps/api-journeys/prisma/seed.ts",
            "tsConfig": "apps/api-journeys/tsconfig.app.json"
          }
        },
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config apps/api-journeys/codegen.yml"
              }
            ]
          }
        },
        "generate-graphql": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "rover subgraph introspect http://localhost:4001/graphql > apps/api-journeys/schema.graphql"
              }
            ]
          }
        }
      }
    },
    "journeys": {
      "root": "apps/journeys",
      "sourceRoot": "apps/journeys",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/journeys",
            "outputPath": "dist/apps/journeys"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "journeys:build",
            "dev": true,
            "hostname": "0.0.0.0",
            "port": 4100
          },
          "configurations": {
            "production": {
              "buildTarget": "journeys:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "journeys:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/journeys"],
          "options": {
            "jestConfig": "apps/journeys/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/journeys/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx apollo client:codegen --config apps/journeys/apollo.config.js --target typescript --outputFlat apps/journeys/__generated__"
              }
            ]
          }
        }
      }
    },
    "journeys-e2e": {
      "root": "apps/journeys-e2e",
      "sourceRoot": "apps/journeys-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/journeys-e2e/cypress.json",
            "tsConfig": "apps/journeys-e2e/tsconfig.e2e.json",
            "devServerTarget": "journeys:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "journeys:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/journeys-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-storybook": {
      "root": "libs/shared/storybook",
      "sourceRoot": "libs/shared/storybook/src",
      "projectType": "library",
      "targets": {
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shared-storybook",
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "shared-util-graphql": {
      "root": "libs/shared/util-graphql",
      "sourceRoot": "libs/shared/util-graphql/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/util-graphql/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/util-graphql"],
          "options": {
            "jestConfig": "libs/shared/util-graphql/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "api-journeys",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
