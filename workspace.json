{
  "version": 2,
  "projects": {
    "api-gateway": {
      "root": "apps/api-gateway",
      "sourceRoot": "apps/api-gateway/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-gateway",
            "main": "apps/api-gateway/src/main.ts",
            "tsConfig": "apps/api-gateway/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-gateway/src/environments/environment.ts",
                  "with": "apps/api-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-gateway:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-gateway/**/*.ts"]
          }
        }
      }
    },
    "api-journeys": {
      "root": "apps/api-journeys",
      "sourceRoot": "apps/api-journeys/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-journeys",
            "main": "apps/api-journeys/src/main.ts",
            "tsConfig": "apps/api-journeys/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-journeys/src/environments/environment.ts",
                  "with": "apps/api-journeys/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-journeys:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-journeys/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-journeys"],
          "options": {
            "jestConfig": "apps/api-journeys/jest.config.js",
            "passWithNoTests": true
          }
        },
        "generate": {
          "executor": "@nx-tools/nx-prisma:generate",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "migrations": {
          "executor": "@nx-tools/nx-prisma:migrations",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config apps/api-journeys/codegen.yml"
              }
            ]
          }
        }
      }
    },
    "journeys": {
      "root": "apps/journeys",
      "sourceRoot": "apps/journeys/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/journeys",
            "index": "apps/journeys/src/index.html",
            "main": "apps/journeys/src/main.tsx",
            "polyfills": "apps/journeys/src/polyfills.ts",
            "tsConfig": "apps/journeys/tsconfig.app.json",
            "assets": [
              "apps/journeys/src/favicon.ico",
              "apps/journeys/src/assets"
            ],
            "styles": ["apps/journeys/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/journeys/src/environments/environment.ts",
                  "with": "apps/journeys/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "journeys:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "journeys:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/journeys/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/journeys"],
          "options": {
            "jestConfig": "apps/journeys/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "journeys-e2e": {
      "root": "apps/journeys-e2e",
      "sourceRoot": "apps/journeys-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/journeys-e2e/cypress.json",
            "tsConfig": "apps/journeys-e2e/tsconfig.e2e.json",
            "devServerTarget": "journeys:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "journeys:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/journeys-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "myapp": {
      "root": "apps/myapp",
      "sourceRoot": "apps/myapp/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/myapp",
            "index": "apps/myapp/src/index.html",
            "main": "apps/myapp/src/main.tsx",
            "polyfills": "apps/myapp/src/polyfills.ts",
            "tsConfig": "apps/myapp/tsconfig.app.json",
            "assets": ["apps/myapp/src/favicon.ico", "apps/myapp/src/assets"],
            "styles": ["apps/myapp/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/myapp/src/environments/environment.ts",
                  "with": "apps/myapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "myapp:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "myapp:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/myapp/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/myapp"],
          "options": {
            "jestConfig": "apps/myapp/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "shared-storybook": {
      "root": "libs/shared/storybook",
      "sourceRoot": "libs/shared/storybook/src",
      "projectType": "library",
      "targets": {
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shared-storybook",
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "shared-util-graphql": {
      "root": "libs/shared/util-graphql",
      "sourceRoot": "libs/shared/util-graphql/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/util-graphql/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/util-graphql"],
          "options": {
            "jestConfig": "libs/shared/util-graphql/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "api-journeys",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
