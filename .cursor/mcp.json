{
  "mcpServers": {
    "nx-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "nx-mcp@latest"
      ]
    },
    "playwright-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "playwright-mcp@latest"
      ]
    },
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    }
  }
}
