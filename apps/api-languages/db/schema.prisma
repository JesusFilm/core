generator client {
  provider = "prisma-client-js"
  output   = env("PRISMA_LOCATION_LANGUAGES")
}

datasource db {
  provider = "postgresql"
  url      = env("PG_DATABASE_URL_LANGUAGES")
}

model Language {
  id        String   @id
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      Json[]
  bcp47     String?
  iso3      String?
}
