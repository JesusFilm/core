generator client {
  provider = "prisma-client-js"
  output   = env("PRISMA_LOCATION")
}

datasource db {
  provider = "postgresql"
  url      = env("PG_DATABASE_URL")
}

model Country {
  id          String     @id
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  name        Json[]
  population  Int
  slug        String     @unique
  latitude    Float
  longitude   Float
  image       String
  continent   Json[]
  languageIds String[]
  languages   Language[]
}

model Language {
  id        String    @id
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  name      Json[]
  bcp47     String?
  iso3      String?
  Countries Country[]
}
