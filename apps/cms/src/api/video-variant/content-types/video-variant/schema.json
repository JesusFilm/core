{
  "kind": "collectionType",
  "collectionName": "video_variants",
  "info": {
    "singularName": "video-variant",
    "pluralName": "video-variants",
    "displayName": "Video Variant",
    "description": "Video variants imported from API Media database"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "remoteId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "video": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::video.video",
      "inversedBy": "video_variants"
    },
    "language": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::language.language",
      "inversedBy": "video_variants"
    },
    "lengthInMilliseconds": {
      "type": "integer"
    },
    "hls": {
      "type": "string"
    },
    "downloads": {
      "type": "component",
      "component": "video-variant.download",
      "repeatable": true
    },
    "edition": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::video-edition.video-edition",
      "inversedBy": "video_variants"
    }
  }
}
