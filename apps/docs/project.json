{
  "root": "apps/docs",
  "projectType": "application",
  "sourceRoot": "apps/docs/src",
  "targets": {
    "build": {
      "executor": "@nx-plus/docusaurus:browser",
      "options": {
        "outputPath": "apps/docs/dist/apps/docs"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "VERCEL_PROJECT_ID=$DOCS_VERCEL_PROJECT_ID npx vercel pull apps/docs --environment=preview --token=$VERCEL_TOKEN",
          "npx vercel build --cwd apps/docs",
          "VERCEL_PROJECT_ID=$DOCS_VERCEL_PROJECT_ID npx vercel deploy apps/docs --prebuilt --token=$VERCEL_TOKEN > apps/docs/.vercel/url.txt"
        ],
        "parallel": false
      },
      "configurations": {
        "production": {
          "commands": [
            "VERCEL_PROJECT_ID=$DOCS_VERCEL_PROJECT_ID npx vercel pull apps/docs --environment=production --token=$VERCEL_TOKEN",
            "npx vercel build --cwd apps/docs",
            "VERCEL_PROJECT_ID=$DOCS_VERCEL_PROJECT_ID npx vercel deploy apps/docs --prebuilt --prod --token=$VERCEL_TOKEN"
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx-plus/docusaurus:dev-server",
      "options": {
        "port": 3000
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/docs/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
