{
  "name": "journeys-admin-pwa",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/journeys-admin-pwa",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx _build journeys-admin-pwa"]
      },
      "configurations": {
        "production": {
          "commands": ["CI=true nx _build journeys-admin-pwa --prod"]
        }
      }
    },
    "_build": {
      "executor": "@nx/remix:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/journeys-admin-pwa"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/remix:serve",
      "options": {
        "buildTarget": "journeys-admin-pwa:_build",
        "dev": true,
        "hostname": "0.0.0.0",
        "port": 3000
      },
      "configurations": {
        "production": {
          "buildTarget": "journeys-admin-pwa:build:production",
          "dev": false
        },
        "development": {
          "buildTarget": "journeys-admin-pwa:_build:development",
          "dev": true
        }
      },
      "defaultConfiguration": "development"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/journeys-admin-pwa"],
      "options": {
        "jestConfig": "apps/journeys-admin-pwa/jest.config.ts"
      }
    }
  }
}
