{
  "name": "journeys-admin-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/journeys-admin-e2e/src",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["journeys-admin"],
  "targets": {
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["playwright test"],
        "cwd": "apps/journeys-admin-e2e"
      }
    },
    "update-snapshots": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["playwright test --update-snapshots"],
        "cwd": "apps/journeys-admin-e2e"
      }
    },
    "debug": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "PWDEBUG=1 xvfb-run playwright test --project=chromium --timeout=0"
        ],
        "cwd": "apps/journeys-admin-e2e"
      }
    },
    "show-report": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["playwright show-report"],
        "cwd": "apps/journeys-admin-e2e"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bunx tsc -b apps/journeys-admin-e2e/tsconfig.json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bunx @biomejs/biome check --diagnostic-level=warn",
        "cwd": "apps/journeys-admin-e2e"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bunx @biomejs/biome check --write",
        "cwd": "apps/journeys-admin-e2e"
      }
    }
  }
}
