{
  "name": "api-analytics",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api-analytics/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/api-analytics",
        "main": "apps/api-analytics/src/main.ts",
        "tsConfig": "apps/api-analytics/tsconfig.app.json",
        "assets": [
          "apps/api-analytics/src/assets",
          {
            "glob": "**/*.graphql",
            "input": "apps/api-analytics/src/app/",
            "output": "./assets"
          },
          {
            "glob": "**/*.graphql",
            "input": "libs/nest/common/src/lib/TranslationModule/",
            "output": "./assets"
          }
        ],
        "generatePackageJson": true,
        "compiler": "tsc",
        "target": "node",
        "webpackConfig": "apps/api-analytics/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/api-analytics/src/environments/environment.ts",
              "with": "apps/api-analytics/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "api-analytics:build",
        "inspect": false
      }
    },
    "watcher": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run ts-node tools/scripts/generate-typings.ts api-analytics --transpileOnly"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/api-analytics"],
      "options": {
        "jestConfig": "apps/api-analytics/jest.config.ts",
        "fileReplacements": [
          {
            "replace": "apps/api-analytics/src/environments/environment.ts",
            "with": "apps/api-analytics/src/environments/environment.testing.ts"
          }
        ]
      }
    },
    "generate-graphql": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rover subgraph introspect http://localhost:4001/graphql > apps/api-analytics/schema.graphql"
      }
    },
    "subgraph-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rover subgraph check API-GATEWAY-0zuda@current --name api-analytics --schema apps/api-analytics/schema.graphql"
      }
    },
    "fetch-secrets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DOPPLER_TOKEN=$DOPPLER_API_ANALYTICS_TOKEN doppler secrets download --no-file --format=env-no-quotes --project api-analytics > apps/api-analytics/.env"
      }
    },
    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "npx prisma generate --schema apps/api-analytics/db/schema.prisma",
          "npm run ts-node apps/api-analytics/src/app/lib/casl/caslPrismaGenerator.ts"
        ]
      }
    },
    "prisma-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bash -c 'npx prisma migrate dev --schema apps/api-analytics/db/schema.prisma --name `date +\"%Y%m%d%H%M%S\"`'"
        ]
      },
      "configurations": {
        "production": {
          "commands": [
            "npx prisma migrate deploy --schema apps/api-analytics/db/schema.prisma"
          ]
        }
      }
    },
    "prisma-reset": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma migrate reset --schema apps/api-analytics/db/schema.prisma --force"
        ]
      }
    },
    "prisma-seed": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npm run ts-node apps/api-analytics/db/seed.ts"]
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma studio --schema apps/api-analytics/db/schema.prisma"
        ]
      }
    },
    "prisma-validate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma validate --schema apps/api-analytics/db/schema.prisma"
        ]
      }
    },
    "prisma-resolve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma migrate resolve --schema apps/api-analytics/db/schema.prisma --applied 20230711193802_20230711193801"
        ]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc -b apps/api-analytics/tsconfig.json"
      }
    },
    "codecov": {
      "executor": "nx:run-commands",
      "options": {
        "command": "codecov -f coverage/apps/api-analytics/cobertura-coverage.xml -F apps.api-analytics"
      }
    },
    "email": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "if lsof -Pi :4601 -sTCP:LISTEN -t>/dev/null; then\nkill -9 $(lsof -Pi :4601 -sTCP:LISTEN -t)\nfi",
          "email dev --dir apps/api-analytics/src/app/emails/templates --port 4601"
        ]
      }
    }
  },
  "tags": []
}
