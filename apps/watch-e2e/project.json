{
  "root": "apps/watch-e2e",
  "sourceRoot": "apps/watch-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "start-server-and-test \"nx serve journeys:serve-all\" 4100 \"nx _e2e watch-e2e\""
          }
        ]
      }
    },
    "_e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/watch-e2e/cypress.json",
        "tsConfig": "apps/watch-e2e/tsconfig.e2e.json",
        "baseUrl": "http://localhost:4100"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/watch-e2e/**/*.{js,ts}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "type-check": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx tsc -b apps/watch-e2e/tsconfig.json"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["journeys"]
}
