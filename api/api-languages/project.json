{
  "name": "api-languages",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "api/api-languages/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "outputPath": "dist/apps/api-languages",
        "main": "api/api-languages/src/index.ts",
        "tsConfig": "api/api-languages/tsconfig.app.json",
        "compiler": "tsc",
        "target": "node",
        "webpackConfig": "api/api-languages/webpack.config.js"
      },
      "configurations": {
        "production": {
          "generatePackageJson": true,
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx _serve api-languages | npx pino-pretty --config .pino-prettyrc.js"
      }
    },
    "_serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "api-languages:build",
        "inspect": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "cache": true,
        "cacheLocation": ".cache/api-languages/eslint",
        "cacheStrategy": "content"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/api/api-languages"],
      "options": {
        "jestConfig": "api/api-languages/jest.config.ts"
      }
    },
    "generate-graphql": {
      "executor": "nx:run-commands",
      "options": {
        "command": "GENERATE_SCHEMA=true npx ts-node -P api/api-languages/tsconfig.app.json -r tsconfig-paths/register api/api-languages/src/schema/index.ts"
      }
    },
    "subgraph-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx hive schema:check api/api-languages/schema.graphql --registry.accessToken $HIVE_TOKEN --github --service api-languages"
      }
    },
    "fetch-secrets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DOPPLER_TOKEN=$DOPPLER_API_LANGUAGES_TOKEN doppler secrets download --no-file --format=env-no-quotes --project api-languages > api/api-languages/.env"
      }
    },
    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma generate --schema api/api-languages/db/schema.prisma"
        ]
      }
    },
    "prisma-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bash -c 'npx prisma migrate dev --schema api/api-languages/db/schema.prisma --name `date +\"%Y%m%d%H%M%S\"`'"
        ],
        "parallel": false
      },
      "configurations": {
        "production": {
          "commands": [
            "npx prisma migrate deploy --schema api/api-languages/db/schema.prisma"
          ]
        }
      }
    },
    "prisma-reset": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma migrate reset --schema api/api-languages/db/schema.prisma --force"
        ]
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma studio --schema api/api-languages/db/schema.prisma"
        ]
      }
    },
    "prisma-validate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma validate --schema api/api-languages/db/schema.prisma"
        ]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc -b api/api-languages/tsconfig.json"
      }
    },
    "codecov": {
      "executor": "nx:run-commands",
      "options": {
        "command": "codecov -f coverage/api/api-languages/cobertura-coverage.xml -F apps.api-languages"
      }
    },
    "ingest": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run ts-node api/api-languages/src/workers/cli.ts big-query"
      }
    },
    "queue-algolia": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run ts-node api/api-languages/src/workers/cli.ts algolia"
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gql.tada generate output --tsconfig api/api-languages/tsconfig.json"
      }
    }
  }
}
