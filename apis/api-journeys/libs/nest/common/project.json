{
  "name": "nest-common",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apis/api-journeys/libs/nest/common/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "cache": true,
        "cacheLocation": ".cache/nest-common/eslint",
        "cacheStrategy": "content"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apis/api-journeys/libs/nest/common"],
      "options": {
        "jestConfig": "apis/api-journeys/libs/nest/common/jest.config.ts"
      }
    },
    "codecov": {
      "executor": "nx:run-commands",
      "options": {
        "command": "codecov -f coverage/apis/api-journeys/libs/nest/common/cobertura-coverage.xml -F apis.api-journeys.libs.nest.common"
      }
    }
  }
}
