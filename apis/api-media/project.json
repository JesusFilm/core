{
  "name": "api-media",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apis/api-media/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "outputPath": "dist/apps/api-media",
        "main": "apis/api-media/src/index.ts",
        "tsConfig": "apis/api-media/tsconfig.app.json",
        "compiler": "tsc",
        "target": "node",
        "webpackConfig": "apis/api-media/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "_serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "api-media:build",
        "inspect": false
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx _serve api-media | bun run pino-pretty --config .pino-prettyrc.js"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "cache": true,
        "cacheLocation": ".cache/api-media/eslint",
        "cacheStrategy": "content"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apis/api-media"],
      "options": {
        "jestConfig": "apis/api-media/jest.config.ts"
      }
    },
    "generate-graphql": {
      "executor": "nx:run-commands",
      "options": {
        "command": "GENERATE_SCHEMA=true bun run ts-node -P apis/api-media/tsconfig.app.json -r tsconfig-paths/register apis/api-media/src/schema/index.ts"
      }
    },
    "subgraph-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run hive schema:check apis/api-media/schema.graphql --registry.accessToken $HIVE_TOKEN --github --service api-media"
      }
    },
    "fetch-secrets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DOPPLER_TOKEN=$DOPPLER_API_MEDIA_TOKEN doppler secrets download --no-file --format=env-no-quotes --project api-media > apis/api-media/.env"
      }
    },
    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "bun run prisma generate --schema apis/api-media/db/schema.prisma"
        ]
      }
    },
    "prisma-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bash -c 'bun run prisma migrate dev --schema apis/api-media/db/schema.prisma --name `date +\"%Y%m%d%H%M%S\"`'"
        ],
        "parallel": false
      },
      "configurations": {
        "production": {
          "commands": [
            "tools/scripts/env-validator.sh --DOPPLER_CONFIG=dev --PG_DATABASE_URL_MEDIA=postgresql://postgres:postgres@db:5432/media?schema=public \"bun run prisma migrate deploy --schema apis/api-media/db/schema.prisma\""
          ]
        }
      }
    },
    "prisma-reset": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tools/scripts/env-validator.sh --DOPPLER_CONFIG=dev --PG_DATABASE_URL_MEDIA=postgresql://postgres:postgres@db:5432/media?schema=public \"bun run prisma migrate reset --schema apis/api-media/db/schema.prisma --force\""
        ]
      }
    },
    "prisma-resolve-squash": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bun run prisma migrate resolve --schema apis/api-media/db/schema.prisma --applied 000000000000_squashed_migrations"
        ]
      }
    },
    "prisma-seed": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["bun run ts-node apis/api-media/db/seed.ts"]
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bun run prisma studio --schema apis/api-media/db/schema.prisma"
        ]
      }
    },
    "prisma-validate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bun run prisma validate --schema apis/api-media/db/schema.prisma"
        ]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run tsc -b apis/api-media/tsconfig.json"
      }
    },
    "codecov": {
      "executor": "nx:run-commands",
      "options": {
        "command": "codecov -f coverage/apis/api-media/cobertura-coverage.xml -F apps.api-media"
      }
    },
    "queue-ingest": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/workers/cli.ts big-query"
      }
    },
    "queue-crowdin": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/workers/cli.ts crowdin"
      }
    },
    "queue-algolia": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/workers/cli.ts algolia"
      }
    },
    "queue-blocklist": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/workers/cli.ts blocklist"
      }
    },
    "queue-seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/workers/cli.ts seed"
      }
    },
    "export-language-slugs": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bun run ts-node apis/api-media/src/lib/exportExistingLanguageSlugs.ts",
          "bun run prettier apis/api-languages/src/__generated__/languageSlugs.ts --write"
        ],
        "parallel": false
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gql.tada generate output --tsconfig apis/api-media/tsconfig.json"
      }
    },
    "data-export": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/workers/cli.ts data-export"
      }
    },
    "data-import": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run ts-node apis/api-media/src/scripts/data-import.ts"
      }
    }
  }
}
